{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "chela",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "close": true,
        "maximize": true,
        "minimize": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "spc.chela.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": false,
        "resizable": false,
        "x": 0,
        "y": 0,
        "width": 2740,
        "height": 1095,
        "transparent": true,
        "title": "chela"
      }
    ]
  }
}
